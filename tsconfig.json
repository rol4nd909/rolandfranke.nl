{
  "extends": "astro/tsconfigs/strict", 
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@components/*": ["src/components/*"],
      "@styles/*": ["src/css/*"],
      "@utils/*": ["src/utils/*"],
      "@assets/*": ["src/assets/*"],
    }
  }
}